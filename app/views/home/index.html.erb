<h1> Listing all posts </h1>
<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th>Created_at</th>
    <th>Updated_at</th>
  </tr>

  <% @posts.each do |post| %> 
    <tr>
      <td><%= post.title %></td>
      <td><%= post.content %></td>
      <td><%= post.created_at %></td>
      <td><%= post.updated_at %></td>
    </tr>
  <% end %> 
</table>
